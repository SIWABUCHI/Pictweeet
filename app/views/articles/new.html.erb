<div class="contents row">
  <%= form_tag('/articles', method: :post) do %>
    <h3>
      投稿する
    </h3>
    <input placeholder="title" type="text" name="title"><br>
    <input placeholder="Image Url" type="text" name="image"><br>
    <textarea cols="30" name="content" placeholder="text" rows="10"></textarea><br>
    <input type="submit" value="SENT">
  <% end %>
</div>
